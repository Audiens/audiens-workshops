{
    "type": "project",
    "license": "proprietary",
    "require": {
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/asset": "5.1.*",
        "symfony/console": "5.1.*",
        "symfony/dotenv": "5.1.*",
        "symfony/flex": "^1.9",
        "symfony/framework-bundle": "5.1.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/security-bundle": "5.1.*",
        "symfony/twig-bundle": "5.1.*",
        "symfony/yaml": "5.1.*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "facile-it/paraunit": "^1.1",
        "symfony/browser-kit": "5.1.*",
        "symfony/phpunit-bridge": "^5.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "platform": {
          "php": "7.4.4",
          "ext-date": "7.4.4",
          "ext-libxml": "7.4.4",
          "ext-openssl": "7.4.4",
          "ext-pcre": "7.4.4",
          "ext-sqlite3":"7.4.4",
          "ext-zlib": "7.4.4",
          "ext-ctype": "7.4.4",
          "ext-curl":"7.4.4",
          "ext-fileinfo": "7.4.4",
          "ext-filter": "7.4.4",
          "ext-ftp": "7.4.4",
          "ext-hash": "7.4.4",
          "ext-iconv": "7.4.4",
          "ext-json": "7.4.4",
          "ext-mbstring": "7.4.4",
          "ext-SPL": "7.4.4",
          "ext-session": "7.4.4",
          "ext-posix": "7.4.4",
          "ext-readline": "7.4.4",
          "ext-Reflection": "7.4.4",
          "ext-standard": "7.4.4",
          "ext-SimpleXML": "7.4.4",
          "ext-Phar": "7.4.4",
          "ext-tokenizer": "7.4.4",
          "ext-xml": "7.4.4",
          "ext-xmlreader": "7.4.4",
          "ext-xmlwriter": "7.4.4",
          "ext-sodium": "7.4.4"

        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Test\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    }
}
